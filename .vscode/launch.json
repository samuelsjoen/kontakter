    "version": "0.2.0",
    "configurations": [
        {
            "type": "edge",
            "request": "launch",
            "name": "localhost (Edge)",
            "url": "https://localhost:49979",
            "webRoot": "${workspaceFolder}/Kontakter.Client"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "localhost (Chrome)",
            "url": "https://localhost:49979",
            "webRoot": "${workspaceFolder}/Kontakter.Client"
        },
        {
            "name": "Launch ASP.NET Backend",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Kontakter.Server/bin/Debug/net9.0/Kontakter.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Kontakter.Server",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
